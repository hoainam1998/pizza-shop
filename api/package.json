    "tsx": "^4.20.4",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "setupFiles": [
      "<rootDir>/apps/share/test/pre-setup/mock/index.ts"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/apps/share/test/jest-extends/index.ts"
    ],
    "roots": [
      "<rootDir>/apps"
    ],
    "moduleNameMapper": {
      "^@share(|/.*)$": "<rootDir>/apps/share/$1",
      "generated/prisma": "<rootDir>/generated/prisma"
    }
  }
