    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/api/main",
    "docker-dev": "docker compose -f docker-compose.dev.yaml --env-file ./.env up -d",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ut:category.create": "jest --watch apps/modules/category/src/__test__/create-category.spec.ts",
    "test:ut:category.get-all": "jest --watch apps/modules/category/src/__test__/get-all-categories.spec.ts",
