    "test:e2e:product.pagination": "jest --watch apps/api/src/product/__test__/pagination.spec.ts",
    "test:e2e:product.get-product": "jest --watch apps/api/src/product/__test__/get-product.spec.ts",
    "test:e2e:product.update": "jest --watch apps/api/src/product/__test__/update-product.spec.ts",
    "test:e2e:product.delete": "jest --watch apps/api/src/product/__test__/delete-product.spec.ts",
    "test:e2e:product": "jest --watch apps/api/src/product/__test__/",
    "test:e2e:category.create": "jest --watch apps/api/src/category/__test__/create-category.spec.ts",
    "test:e2e:category.delete": "jest --watch apps/api/src/category/__test__/delete-category.spec.ts",
    "test:e2e:category.pagination": "jest --watch apps/api/src/category/__test__/pagination.spec.ts",
    "test:e2e:category.get-detail": "jest --watch apps/api/src/category/__test__/get-detail.spec.ts",
